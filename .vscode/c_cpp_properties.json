{
  "version": 4,
  "configurations": [
    {
      "name": "Arduino",
      "compilerPath": "/usr/bin/g++",
      "intelliSenseMode": "gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "/home/vscode/.arduino15/packages/**/cores/arduino",
        "/home/vscode/.arduino15/packages/**/variants/**",
        "/home/vscode/.arduino15/packages/**/libraries/**",
        "/home/vscode/Arduino/libraries/**"
      ],
      "cStandard": "c11",
      "cppStandard": "c++17",
      "defines": [
        "ARDUINO=10607",
        "__cplusplus=201103L"
      ]
    }
  ]
}
